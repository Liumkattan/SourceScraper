{
    "name": "vidzi-scrapper",
    "version": "0.1.0",
    "description": "A scrapper for vidzi",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScrapper.git"
    },
    "dependencies": {
        "sourcescrapper-core": "^0.2.0",
        "sourcescrapper-puppeteer-runner": "^0.1.0"
    },
    "devDependencies": {
        "@types/node": "^10.5.2",
        "npm-run-all": "^4.1.3",
        "ncu": "^0.2.1",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.3.1",
        "typedoc": "^0.11.1",
        "typescript": "^2.9.2"
    },
    "scripts": {
        "test": "npm-run-all lint build",
        "lint": "tslint -p .",
        "lint-fix": "tslint -p . --fix",
        "build": "tsc",
        "gen-doc": "typedoc --out ./docs ./lib/ --ignoreCompilerErrors --target ES6 --name \"SourceScrapper Vidzi\" --excludePrivate --module commonjs --mode file --externalPattern node_modules --excludeExternals",
        "update-dep": "ncu -a && npm i"
    },
    "keywords": [
        "sourcescrapper",
        "source",
        "scrapper",
        "extractor",
        "vidzi",
        "vidzi.tv",
        "vidzi.online",
        "vidzi.nu"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/OpenByteDev/SourceScrapper/issues"
    },
    "homepage": "https://github.com/OpenByteDev/SourceScrapper#readme"
}

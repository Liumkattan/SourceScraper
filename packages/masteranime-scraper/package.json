{
    "name": "masteranime-scraper",
    "version": "1.0.7",
    "description": "A scraper for Masteranime",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScraper.git"
    },
    "dependencies": {
        "jsonic": "^0.3.1",
        "source-scraper-core": "^1.1.3",
        "source-scraper-html-runner": "^1.0.6"
    },
    "devDependencies": {
        "@types/jsonic": "^0.3.0",
        "@types/mocha": "^5.2.6",
        "@types/node": "^11.10.4",
        "mocha": "^6.0.2",
        "npm-run-all": "^4.1.5",
        "source-scraper-test-utils": "^1.1.3",
        "ts-node": "^8.0.2",
        "ts-typie": "^1.1.1",
        "tslint": "^5.13.1",
        "tslint-eslint-rules": "^5.4.0",
        "typedoc": "^0.14.2",
        "typescript": "^3.3.3333"
    },
    "scripts": {
        "ci-test": "npm-run-all lint build test",
        "test": "mocha test/*.spec.ts",
        "lint": "tslint -p .",
        "lint-fix": "tslint -p . --fix",
        "build": "tsc",
        "gen-doc": "typedoc --out ./docs ./lib/ --ignoreCompilerErrors --target ES6 --name \"masteranime-scraper\" --excludePrivate --module commonjs --mode file --externalPattern node_modules --excludeExternals"
    },
    "keywords": [
        "source-scraper",
        "source",
        "scrap",
        "scraper",
        "scrapers",
        "scraping",
        "extractor",
        "masteranime",
        "masterani.me",
        "masterani"

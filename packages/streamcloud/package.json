{
    "name": "streamcloud-scrapper",
    "version": "0.3.2",
    "description": "A scrapper for Streamcloud",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScrapper.git"
    },
    "dependencies": {
        "newline-remove": "^1.0.2",
        "query-string": "^6.1.0",
        "regexp.execall": "^1.0.2",
        "sourcescrapper-core": "^0.5.11",
        "sourcescrapper-html-runner": "^0.2.5"
    },
    "devDependencies": {
        "@types/newline-remove": "^1.0.0",
        "@types/node": "^10.5.6",
        "@types/query-string": "^6.1.0",
        "npm-run-all": "^4.1.3",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.3.1",
        "typedoc": "^0.11.1",
        "typescript": "^3.0.1"
    },
    "scripts": {
        "ci-test": "npm test",
        "test": "npm-run-all lint build",
        "lint": "tslint -p .",
        "lint-fix": "tslint -p . --fix",
        "build": "tsc",
        "gen-doc": "typedoc --out ./docs ./lib/ --ignoreCompilerErrors --target ES6 --name \"Streamcloud Scrapper\" --excludePrivate --module commonjs --mode file --externalPattern node_modules --excludeExternals"
    },
    "keywords": [
        "sourcescrapper",
        "source",
        "scrapper",
        "extractor",
        "streamcloud",
        "streamcloud.eu"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/OpenByteDev/SourceScrapper/issues"
    },
    "homepage": "https://github.com/OpenByteDev/SourceScrapper#readme"

{
    "name": "streamcloud-scrapper",
    "version": "0.4.3",
    "description": "A scrapper for Streamcloud",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScrapper.git"
    },
    "dependencies": {
        "newline-remove": "^1.0.2",
        "object-merge": "^2.5.1",
        "query-string": "^6.1.0",
        "regexp.execall": "^1.0.2",
        "sourcescrapper-core": "^0.6.3",
        "sourcescrapper-html-runner": "^0.3.3"
    },
    "devDependencies": {
        "@types/newline-remove": "^1.0.0",
        "@types/node": "^10.7.1",
        "@types/query-string": "^6.1.0",
        "npm-run-all": "^4.1.3",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typedoc": "^0.12.0",
        "typescript": "^3.1.0-dev.20180817"
    },
    "scripts": {
        "ci-test": "npm test",
        "test": "npm-run-all lint build",
        "lint": "tslint -p .",
        "lint-fix": "tslint -p . --fix",
        "build": "tsc",
        "gen-doc": "typedoc --out ./docs ./lib/ --ignoreCompilerErrors --target ES6 --name \"Streamcloud Scrapper\" --excludePrivate --module commonjs --mode file --externalPattern node_modules --excludeExternals"

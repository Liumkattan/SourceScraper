    "keywords": [
        "sourcescrapper",
        "source",
        "scrapper",
        "extractor",
        "streamcloud",
        "streamcloud.eu"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/OpenByteDev/SourceScrapper/issues"

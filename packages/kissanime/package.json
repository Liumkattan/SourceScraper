    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScrapper.git"
    },
    "dependencies": {
        "normalize-url": "^3.2.0",
        "random-number": "0.0.9",
        "sourcescrapper-core": "^0.6.4",
        "sourcescrapper-puppeteer-runner": "^0.5.4",

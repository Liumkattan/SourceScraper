    },
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.7.1",
        "@types/normalize-url": "^1.9.1",
        "@types/random-number": "0.0.0",
        "@types/urlparser": "^0.2.0",
        "chai": "^4.1.2",
        "mocha": "^5.2.0",
        "npm-run-all": "^4.1.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typedoc": "^0.12.0",
        "typescript": "^3.1.0-dev.20180818"
    },
    "scripts": {

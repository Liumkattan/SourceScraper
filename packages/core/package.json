{
    "name": "sourcescrapper-core",
    "version": "0.6.3",
    "description": "Core files for sourcescrapper packages.",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",

{
    "name": "sourcescrapper",
    "version": "1.1.0",
    "description": "Scrap the sources from your favorite video streaming sites.",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScrapper.git"
    },
    "dependencies": {
        "gogoanime-scrapper": "^0.2.0",
        "kissanime-scrapper": "^0.3.0",
        "masteranime-scrapper": "^0.5.0",
        "mp4upload-scrapper": "^0.2.0",
        "openload-scrapper": "^0.4.0",
        "rapidvideo-scrapper": "^0.4.0",
        "sourcescrapper-core": "^0.6.0",
        "streamango-scrapper": "^0.4.0",
        "streamcloud-scrapper": "^0.4.0",
        "streammoe-scrapper": "^0.2.0",
        "vidstreaming-scrapper": "^0.4.0",
        "vidzi-scrapper": "^0.5.0"
    },
    "devDependencies": {
        "@types/node": "^10.5.7",
        "npm-run-all": "^4.1.3",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.3.1",
        "typedoc": "^0.11.1",
        "typescript": "^3.0.1"
    },
    "scripts": {
        "ci-test": "npm run test",
        "test": "npm-run-all lint build",
        "lint": "tslint -p .",
        "lint-fix": "tslint -p . --fix",
        "build": "tsc",
        "gen-doc": "typedoc --out ./docs ./lib/ --ignoreCompilerErrors --target ES6 --name \"SourceScrapper\" --excludePrivate --module commonjs --mode file --externalPattern node_modules --excludeExternals"
    },
    "keywords": [
        "sourcescrapper",
        "source",
        "scrapper",
        "extractor"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/OpenByteDev/SourceScrapper/issues"
    },
    "homepage": "https://github.com/OpenByteDev/SourceScrapper#readme"
}

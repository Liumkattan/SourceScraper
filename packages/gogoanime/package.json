{
    "name": "gogoanime-scrapper",
    "version": "0.2.2",
    "description": "A scrapper for Gogoanime",
    "author": "OpenByte <development.openbyte@gmail.com>",
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/OpenByteDev/SourceScrapper.git"
    },
    "dependencies": {
        "sourcescrapper-core": "^0.6.2",
        "sourcescrapper-dom-runner": "^0.6.2"
    },
    "devDependencies": {
        "@types/chai": "^4.1.4",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.7.1",
        "chai": "^4.1.2",
        "mocha": "^5.2.0",
        "npm-run-all": "^4.1.3",
        "ts-node": "^7.0.1",
        "tslint": "^5.11.0",
        "tslint-eslint-rules": "^5.4.0",
        "typedoc": "^0.12.0",
        "typescript": "^3.1.0-dev.20180817"
    },
    "scripts": {
        "ci-test": "npm-run-all lint build test",
        "test": "mocha --timeout 60000 -r ts-node/register test/*.spec.ts",
        "lint": "tslint -p .",
        "lint-fix": "tslint -p . --fix",
        "build": "tsc",
        "gen-doc": "typedoc --out ./docs ./lib/ --ignoreCompilerErrors --target ES6 --name \"Gogoanime Scrapper\" --excludePrivate --module commonjs --mode file --externalPattern node_modules --excludeExternals"
    },
    "keywords": [
        "sourcescrapper",
        "source",
        "scrapper",
        "extractor",
        "gogoanime",
        "gogoanime.se",
        "gogoanime.sh"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/OpenByteDev/SourceScrapper/issues"
    },
    "homepage": "https://github.com/OpenByteDev/SourceScrapper#readme"
}

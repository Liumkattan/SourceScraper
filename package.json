    },
    "dependencies": {
        "axios": "^0.18.0",
        "htmlstringparser": "^0.2.0",
        "is-travis": "^2.0.0",
        "newline-remove": "^1.0.2",
        "normalize-url": "^3.1.0",
        "object-literal-string-to-object": "^0.2.2",
        "object-merge": "^2.5.1",
        "puppeteer": "^1.5.0",
        "query-string": "^6.1.0",
        "random-number": "^0.0.9",
        "typeof": "^1.0.0",
        "urlparser": "^0.3.9"
    },
    "devDependencies": {
        "@types/newline-remove": "^1.0.0",
        "@types/node": "^10.5.2",
        "@types/normalize-url": "^1.9.1",
        "@types/object-merge": "^2.5.0",
        "@types/puppeteer": "^1.5.0",
        "@types/query-string": "^6.1.0",
        "@types/random-number": "^0.0.0",
        "@types/typeof": "^1.0.0",
        "@types/urlparser": "^0.2.0",
        "tslint": "^5.10.0",
        "typedoc": "^0.11.1",
        "typescript": "^2.9.2"
